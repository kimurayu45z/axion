extern "C"
    fn malloc(size: usize) -> Ptr[{}] with Unsafe, Alloc
    fn realloc(ptr: Ptr[{}], size: usize) -> Ptr[{}] with Unsafe, Alloc
    fn free(ptr: Ptr[{}]) with Unsafe, Alloc
    fn memmove(dst: Ptr[{}], src: Ptr[{}], n: usize) -> Ptr[{}] with Unsafe
    fn memcpy(dst: Ptr[{}], src: Ptr[{}], n: usize) -> Ptr[{}] with Unsafe
    fn memcmp(a: Ptr[{}], b: Ptr[{}], n: usize) -> i32 with Unsafe
