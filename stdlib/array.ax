pub struct Array[T]
    _ptr: i64
    _len: i64
    _cap: i64

impl[T] Array[T]
    fn new() -> Array[T]
        let a: Array[T] = Array #{_ptr: 0, _len: 0, _cap: 0}
        a

    fn len(self) -> i64
        self._len

    fn is_empty(self) -> bool
        self._len == 0

    fn push(mut self, item: T)
        {}

    fn pop(mut self) -> T
        {}

    fn first(self) -> T
        {}

    fn last(self) -> T
        {}

    fn drop(mut self)
        {}
